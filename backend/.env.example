PORT=5000
# Use the direct (5432) Postgres connection for migrations; pooled (6543) may cause DDL issues.
DATABASE_URL=postgresql://user:password@host:5432/dbname
JWT_SECRET=replace_with_a_strong_random_value
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GEMINI_API_KEY=your_gemini_api_key
# Set to true for Supabase/managed DBs that require SSL
DB_SSL=true
FRONTEND_ORIGINS=http://localhost:5173,https://pentabot.vercel.app
